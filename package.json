{
  "name": "@ng2react/support",
  "version": "1.0.0-rc.2",
  "description": "Support functions for mixing AngularJS and React Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxbilbow/ng2react-support.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/index.js",
      "node": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "preversion": "npm run build && npm run lint && npm run test",
    "prepublishOnly": "npm run build",
    "lint": "eslint \"**/*.{ts,cjs,mjs,tsx}\"",
    "prebuild": "rimraf dist",
    "build": "tsc --build src/lib/tsconfig.json",
    "pretest": "rimraf .reports",
    "test": "jest --config=\"./src/test/jest.config.cjs\""
  },
  "author": "Max Bilbow",
  "license": "The Unlicense",
  "peerDependencies": {
    "angular": "1",
    "lodash": "x",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "peerDependenciesMeta": {
    "angular": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@tsconfig/node18": "^2.0.0",
    "@types/angular": "x",
    "@types/angular-translate": "x",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "5",
    "@typescript-eslint/parser": "5",
    "eslint": "^8.34.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.1.0",
    "typescript": "5"
  }
}
